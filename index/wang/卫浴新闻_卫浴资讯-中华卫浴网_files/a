﻿//专题内容点击量，一般用于关注数
function special_item_like(elem, itemid){
	var curhits=parseInt($('#'+elem+itemid).html());
	$.ajax({
		type: "get",
		cache:false,
		async: true,
		dataType: "jsonp",
		jsonp: "callback",
		url: DTPath+"ajax.php",
		data: "action=special&mod=like&itemid="+itemid,
		success: function(data){
			if(data.status=='success'){
				$('#'+elem+itemid).html(curhits+1)
			}
		}
	})
}
