var myalert = function myalert(msg){
	$.dialog({
		width: '400px',
		height:'150',
		title:'消息提醒',
		content: msg,
		icon: 'success.gif',
		lock: true,
		max: false,
		min: false,
		zIndex:2000
	});
}
